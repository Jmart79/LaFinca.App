        <StackLayout>
            <Label Text="Delete Item Page" />
            <Entry x:Name="DeleteItemInput" Placeholder="Item name"/>
            <Button Text="Delete" Clicked="DeleteItemClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>